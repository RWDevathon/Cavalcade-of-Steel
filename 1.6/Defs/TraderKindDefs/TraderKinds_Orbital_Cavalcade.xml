<?xml version="1.0" encoding="UTF-8"?>
<Defs>

    <TraderKindDef>
        <defName>ABF_TraderKind_Synstruct_Cavalcade_Orbital</defName>
        <label>cavalcade trader</label>
        <faction>ABF_Faction_Synstruct_Cavalcade</faction>
        <orbital>true</orbital>
        <stockGenerators>
            <!-- Resources -->
            <li Class="StockGenerator_SingleDef">
                <thingDef>Silver</thingDef>
                <countRange>1600~3200</countRange>
            </li>
            <li Class="StockGenerator_SingleDef">
                <thingDef>ComponentIndustrial</thingDef>
                <countRange>45~90</countRange>
            </li>
            <li Class="StockGenerator_SingleDef">
                <thingDef>ComponentSpacer</thingDef>
                <countRange>0~12</countRange>
            </li>
            <li Class="StockGenerator_SingleDef">
                <thingDef>Steel</thingDef>
                <countRange>500~800</countRange>
            </li>
            <li Class="StockGenerator_SingleDef">
                <thingDef>Plasteel</thingDef>
                <countRange>250~500</countRange>
            </li>
            <li Class="StockGenerator_SingleDef">
                <thingDef>Uranium</thingDef>
                <countRange>120~300</countRange>
            </li>
            <li Class="StockGenerator_SingleDef">
                <thingDef>Chemfuel</thingDef>
                <countRange>500~1200</countRange>
            </li>
            <li Class="StockGenerator_SingleDef">
                <thingDef>Neutroamine</thingDef>
                <countRange>100~240</countRange>
            </li>
            <li Class="StockGenerator_ReinforcedBarrels">
                <countRange>1~4</countRange>
            </li>
            <li Class="StockGenerator_SingleDef">
                <thingDef>Synthread</thingDef>
                <countRange>200~600</countRange>
            </li>
            <li Class="StockGenerator_SingleDef">
                <thingDef>ABF_Thing_Synstruct_RepairStim</thingDef>
                <countRange>30~60</countRange>
            </li>
            <li Class="StockGenerator_SingleDef">
                <thingDef>ABF_Thing_Synstruct_RestructKitDrone</thingDef>
                <countRange>8~20</countRange>
            </li>

            <!-- Books -->
            <li Class="StockGenerator_SingleDef">
                <thingDef>Schematic</thingDef>
                <countRange>1~1</countRange>
            </li>

            <!-- Stimulators -->
            <li Class="StockGenerator_Category">
                <categoryDef>ABF_ThingCategory_Synstruct_Stimulators</categoryDef>
                <thingDefCountRange>1~4</thingDefCountRange>
                <totalPriceRange>1400~3000</totalPriceRange>
            </li>

            <!-- Weapons -->
            <li Class="StockGenerator_MarketValue">
                <tradeTag>WeaponRanged</tradeTag>
                <countRange>6~12</countRange>
            </li>
            <li Class="StockGenerator_Category">
                <categoryDef>WeaponsMelee</categoryDef>
                <thingDefCountRange>6~8</thingDefCountRange>
                <countRange>1~3</countRange>
            </li>
            <li Class="StockGenerator_Tag">
                <tradeTag>MortarShell</tradeTag>
                <thingDefCountRange>1~3</thingDefCountRange>
                <countRange>10~40</countRange>
                <excludedThingDefs>
                    <li MayRequire="Ludeon.RimWorld.Anomaly">Shell_Deadlife</li>
                </excludedThingDefs>
            </li>

            <!-- Implants -->
            <li Class="StockGenerator_Category">
                <categoryDef>ABF_ThingCategory_Synstruct_BodyParts</categoryDef>
                <thingDefCountRange>3~9</thingDefCountRange>
                <countRange>1~4</countRange>
            </li>

            <!-- Techprints -->
            <li Class="StockGenerator_Techprints">
                <countChances>
                    <li>
                        <count>3</count>
                        <chance>1</chance>
                    </li>
                </countChances>
            </li>

            <!-- Buildings -->
            <li Class="StockGenerator_Category">
                <categoryDef>BuildingsSecurity</categoryDef>
                <thingDefCountRange>2~3</thingDefCountRange>
                <countRange>1~4</countRange>
                <excludedThingDefs>
                    <li MayRequire="Ludeon.RimWorld.Anomaly">TrapIED_Deadlife</li>
                </excludedThingDefs>
            </li>

            <!-- Exotic -->
            <!-- none -->

            <!-- Drones -->
            <li Class="ArtificialBeings.StockGenerator_Colonists">
                <respectPopulationIntent>false</respectPopulationIntent>
                <pawnKindDefs>
                    <li>ABF_PawnKind_Synstruct_Cavalcade_Grunt_Requisition</li>
                    <li>ABF_PawnKind_Synstruct_Cavalcade_Simpleton_Requisition</li>
                    <li>ABF_PawnKind_Synstruct_Cavalcade_Commoner_Requisition</li>
                    <li>ABF_PawnKind_Synstruct_Cavalcade_Warrior_Requisition</li>
                    <li>ABF_PawnKind_Synstruct_Cavalcade_Bishop_Requisition</li>
                </pawnKindDefs>
                <countRange>0~5</countRange>
            </li>

            <!-- Animals -->
            <li Class="StockGenerator_Animals">
                <maxWildness>0.70</maxWildness>
                <tradeTagsSell>
                    <li>ABF_SynstructAnimal</li>
                </tradeTagsSell>
                <tradeTagsBuy>
                    <li>ABF_SynstructAnimal</li>
                </tradeTagsBuy>
                <kindCountRange>3</kindCountRange>
                <countRange>4~8</countRange>
            </li>

            <!-- Buying -->
            <li Class="StockGenerator_BuyExpensiveSimple" />
            <li Class="StockGenerator_BuyTradeTag"><tag>HiTechArmor</tag></li>
        </stockGenerators>
    </TraderKindDef>

</Defs>