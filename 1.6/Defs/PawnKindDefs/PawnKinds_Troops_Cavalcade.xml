    <PawnKindDef ParentName="ABF_PawnKind_Synstruct_Cavalcade_AbstractBase">
        <defName>ABF_PawnKind_Synstruct_Cavalcade_Simpleton_SwarmerRanged</defName>
        <label>swarm ranger</label>
        <race>ABF_Thing_Synstruct_Simpleton</race>
        <combatPower>40</combatPower>
        <aiAvoidCover>true</aiAvoidCover>
        <itemQuality>Awful</itemQuality>
        <weaponTags>
            <li>Gun</li>
        </weaponTags>
        <weaponMoney>140~140</weaponMoney>
        <apparelMoney>0</apparelMoney>
        <apparelAllowHeadgearChance>0</apparelAllowHeadgearChance>
        <gearHealthRange>0.25~0.75</gearHealthRange>
        <modExtensions>
            <li Class="ArtificialBeings.ABF_ArtificialPawnKindExtension">
                <pawnState>Reprogrammable</pawnState>
                <requiredDirectives>
                    <li>ABF_Directive_Synstruct_Martyr</li>
                </requiredDirectives>
                <discretionaryComplexity>0~4</discretionaryComplexity>
            </li>
        </modExtensions>
    </PawnKindDef>

